#lang sicp
(#%require sicp-pict)
;
(define (make_rect xcor_vect ycor_vect)
  (list  xcor_vect ycor_vect))
(define (xcor_vect v)
  (car v))
(define (ycor_vect v)
  (cadr v))
(make_rect 1 2)
(define (add_vect vect1 vect2)
  (make_rect (+ (xcor_vect vect1)
                (xcor_vect vect2))
             (+ (ycor_vect vect1)
                (ycor_vect vect2))))
(define (sub_vect vect1 vect2)
  (make_rect (- (xcor_vect vect1)
                (xcor_vect vect2))
             (- (ycor_vect vect1)
                (ycor_vect vect2))))
(define (scale_vect scale vect)
  (make_rect (* (xcor_vect vect)
                scale)
             (* (ycor_vect vect)
                scale)))


